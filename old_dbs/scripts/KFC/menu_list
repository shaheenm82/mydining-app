        function bump_up(item_id){
            jQuery.get('/console/bump/'+item_id+'/up/');
            document.location = document.location;
        }

        function bump_down(item_id){
            jQuery.get('/console/bump/'+item_id+'/down/');
            document.location = document.location;
        }
    </script>

	
	
	
