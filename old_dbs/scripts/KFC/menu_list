<script type="text/javascript">
function bind_registration_form() {
    $('#frmRegister').ajaxForm(options);
}

var options = { 
    success: function(response_text, status_text, xhr, form_elem) {
        try {
            var response_json = JSON.parse(response_text);
            window.location = response_json.redirect_url;
        } catch(e) {
            $('div.sign_up_pop').replaceWith(response_text);
			$("div.sign_up_pop").show();
            bind_registration_form();
            $("#frmRegister input[type='submit']").attr("value", "Woops, Please Correct");
	        // reset styling updates
            $("div.sign_up_pop").live("mouseleave", (function() {
        	    $(this).hide();
        	}));
		    $('.actions li.sign_up div.why_join_inside ul li:first').css({"border-left":"0"});
		    $('.actions li.sign_up div.why_join_inside ul li:last').css({"border-right":"0"});
        }
    }, 
    beforeSubmit: function() {
        $("#frmRegister input[type='submit']").attr("disabled", true);
        $("#frmRegister input[type='submit']").attr("value", "Registering, Please Wait...");
    }
};

$(function() {
    bind_registration_form();
});
</script>
