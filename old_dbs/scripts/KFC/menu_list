<script type="text/javascript">
function bind_login_form() {
    $('#frmLogin').ajaxForm(login_form_options);
}

var login_form_options = { 
    success: function(response_text, status_text, xhr, form_elem) {
        try {
            var response_json = JSON.parse(response_text);
            window.location = response_json.redirect_url;
        } catch(e) {
            $('div.sign_in_pop').replaceWith(response_text);
			$("div.sign_in_pop").show();
            bind_login_form();
            $("#frmLogin input[type='submit']").attr("value", "Please Correct");
	        // reset styling updates
            $("div.sign_in_pop").live("mouseleave", (function() {
        	    $(this).hide();
        	}));
        }
    }, 
    beforeSubmit: function() {
        $("#frmLogin input[type='submit']").attr("disabled", true);
        $("#frmLogin input[type='submit']").attr("value", "Please Wait...");
    }
};

$(function() {
    bind_login_form();
});
</script>
