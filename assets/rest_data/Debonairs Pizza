    <form id="OrderPopupForm" method="post" action="/Store/SelectAStore">
    </form>
	
	<script type="text/javascript">
		Namespace.Manager.Register('DWP.Store.SelectAStore');

		$(function () {
		    var provinces = $("#provinces");
		    var areas = $("#areas");

		    //areas.attr('disabled', 'disabled');

		    provinces.change(function () {

		        var url = '/Store/SelectAStore?provinceId=-1';
		        url = url.replace('-1', provinces.val());
		        window.location.href = url;
		    });

		    areas.change(function () {

		        var url = '/Store/SelectAStore?provinceId=-1&areaId=-2';
		        url = url.replace('-1', provinces.val());
		        url = url.replace('-2', areas.val());
		        window.location.href = url;
		    });

		});
		
		DWP.Store.SelectAStore.ClearStoresList = function()
		{
			$("#storelist").html('');
		}
		
		DWP.Store.SelectAStore.UpdateAreasDropdown = function(provinceId)
		{
			$('#areas').find('option').remove();
			$('<option value="0">Any Area</option>').appendTo($('#areas'));
			$('#areas').attr('disabled', 'disabled');
			$('#suburbs').attr('disabled', 'disabled');
			
			$.post('/Store/GetAreas',
							{ provinceId: provinceId },
							function(data)
							{
								$('#areas').find('option').remove();
								$.each(eval(data),
									function()
									{
										$('<option value="'+ this.Value + '">' + this.Text + '</option>').appendTo($('#areas'));
										$('#areas').removeAttr('disabled');
									}
								);
							}
					);
		}
		
		
		DWP.Store.SelectAStore.GetStoresForSuburb = function(suburbId)
		{
			$.post('/Store/GetStores',
					{ suburbId: suburbId },
					function(data)
					{										
						$("#storelist").html(data);
					}
			);
		}
		
		DWP.Store.SelectAStore.GetStoresForArea = function(areaId)
		{
			$.post('/Store/GetStores',
					{ areaId: areaId },
					function(data)
					{										
						$("#storelist").html(data);
					}
			);
		}
		
		DWP.Store.SelectAStore.GetStoresForProvince = function(provinceId)
		{
			$.post('/Store/GetStores',
					{ provinceId: provinceId },
					function(data)
					{										
						$("#storelist").html(data);
					}
			);
		}
		
		DWP.Store.SelectAStore.SelectProvince = function(provinceId)
		{
			$('#provinces').val(provinceId);
			$('#flash-map').hide();
			
			DWP.Store.SelectAStore.UpdateAreasDropdown(provinceId);
			DWP.Store.SelectAStore.GetStoresForProvince(provinceId);
		}
		
		DWP.Store.SelectAStore.OrderPopup = function(storeId)
		{
			var url = '/Store/GetOrderPopup/' + storeId;
			$.get(url,
					function(data)
					{
						//Updates orderpopup data
						$('#orderpopup').html(data);
						
						//Triggers fancybox popup
						$('#orderpopup-trigger').trigger('click');
					}
			);
		}
		            
		DWP.Store.SelectAStore.SubmitOrderPopup = function(storeId, suburbId, isCollection)
		{
			$.fancybox.close();
			window.location.href = '/Store/Select?id=' + storeId + '&isCollection=' + isCollection + '&suburbId=' + suburbId;
		}     
		
		DWP.Store.SelectAStore.SubmitDeliveryOrderPopupStoreOnly = function(storeId, suburbId, isCollection)
		{
			$.fancybox.close();
			var suburbId = $('#SelectedSuburb:checked').val();
			window.location.href = '/Store/Select?id=' + storeId + '&isCollection=' + false + '&suburbId=' + suburbId;			
		}

		DWP.Store.SelectAStore.SubmitOrderPopupStoreOnly = function (storeId, isCollection) {
			$.fancybox.close();
			var url = '/Store/Select?id=' + storeId + '&isCollection=' + isCollection;
			window.location.href = url;
		}
		
		DWP.Store.SelectAStore.Deliver = function(storeId)
		{
			var suburbSelected = false;
			
			$('.SelectedSuburbRadio').each(function(index)
			{
				if ($(this).attr('checked'))
				{
					suburbSelected = true;
				}
			});
			
			if (!suburbSelected)
			{
				$('#deliverorderlink').bind('click', DWP.Store.SelectAStore.DisableLink);
				$('#suburbError').show();
			}
			else
			{
				$('#suburbError').hide();
				DWP.Store.SelectAStore.SubmitDeliveryOrderPopupStoreOnly(storeId, false)
			}
		}
		
		DWP.Store.SelectAStore.DisableLink = function(e) 
		{
			// cancels the event
			e.preventDefault();

			return false;
		}
		
	</script>

			<div class="skyscraper"></div>
		</div>  
	</div>

