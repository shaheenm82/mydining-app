<script type="text/javascript">
	$(document).ready(function () {

		$('.searchmenu').hide();

		$("a.select").mouseover(function () {
			$(".searchmenu").show()
		});

		$(".searchmenu").mouseover(function () {
			$(".searchmenu").show()
		});

		$(".searchmenu").mouseout(function () {
			$(".searchmenu").hide()
		});

		$("a.select").mouseout(function () {
			$(".searchmenu").hide()
		});
	});


	var DWPMenu = {
		showAbout: function () { $('#about-div').show(); $("li.about").addClass("active"); },
		hideAbout: function () { $('#about-div').hide(); $("li.about").removeClass("active"); }
	}

	var hoverIntentConfig = { over: DWPMenu.showAbout, timeout: 1, out: DWPMenu.hideAbout };
	$("li.about").hoverIntent(hoverIntentConfig);
</script>		</div>
