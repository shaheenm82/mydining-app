	<div id="storelist">
	
	<script language="javascript">

		Namespace.Manager.Register('DWP.Store.StoreList');

		$(function() {

		$("a#orderpopup-trigger").fancybox({
		'autoDimensions'	: false,
		'showCloseButton'	: false,
		'transitionIn'		: 'fade',
		'transitionOut'		: 'fade',
		'height'         	: 500,
		'width'         	: 680
		});

		});
		
		DWP.Store.StoreList.OrderPopupTrack = function(storeId, storeName)
		{
			_gaq.push(['_trackEvent', 'Store', 'Promo Click Details', storeName]);
			DWP.Store.SelectAStore.OrderPopup(storeId);
		}
		
		DWP.Store.StoreList.StoreDetailsPopup = function(storeId, storeName, isOnline)
		{
			if (isOnline == 'True')
			{
				$("a#detailspopup-trigger").fancybox({
				'autoDimensions'	: false,
				'showCloseButton'	: false,
				'transitionIn'		: 'fade',
				'transitionOut'		: 'fade',
				'height'         	: 350,
				'width'         	: 370
				});
			}
			else
			{
				$("a#detailspopup-trigger").fancybox({
				'autoDimensions'	: false,
				'showCloseButton'	: false,
				'transitionIn'		: 'fade',
				'transitionOut'		: 'fade',
				'height'         	: 350,
				'width'         	: 370
				});
			}

		_gaq.push(['_trackEvent', 'Store', 'View Details', storeName]);

		var url = '/Store/GetStoreDetailsPopup/' + storeId;
		$.get(url,
		function(data)
		{
		//Updates orderpopup data
		$('#orderpopup').html(data);

		//Triggers fancybox popup
		$('#detailspopup-trigger').trigger('click');
		});
		}

	</script>
	
